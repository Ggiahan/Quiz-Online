    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HandleAnswerServlet</servlet-name>
        <url-pattern>/HandleAnswerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SummaryofScoreServlet</servlet-name>
        <url-pattern>/SummaryofScoreServlet</url-pattern>
    </servlet-mapping>   
    <servlet-mapping>
        <servlet-name>SearchHistoryServlet</servlet-name>
        <url-pattern>/SearchHistoryServlet</url-pattern>
    </servlet-mapping>
