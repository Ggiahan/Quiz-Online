    </servlet>
    <servlet>
        <servlet-name>DeleteQuizServlet</servlet-name>
        <servlet-class>hanlg.servlet.DeleteQuizServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>hanlg.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HandlingAnswerServlet</servlet-name>
        <servlet-class>hanlg.servlet.HandlingAnswerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentSevlet</servlet-name>
        <servlet-class>hanlg.servlet.GetListQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListquestionServlet</servlet-name>
        <servlet-class>hanlg.servlet.ListquestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HandleAnswerServlet</servlet-name>
        <servlet-class>hanlg.servlet.HandleAnswerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SummaryofScoreServlet</servlet-name>
        <servlet-class>hanlg.servlet.SummaryofScoreServlet</servlet-class>
    </servlet>
   
    <servlet>
        <servlet-name>SearchHistoryServlet</servlet-name>
        <servlet-class>hanlg.servlet.SearchHistoryServlet</servlet-class>
