        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>hanlg.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationSevlet</servlet-name>
        <servlet-class>hanlg.servlet.RegistrationSevlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SHA256</servlet-name>
        <servlet-class>hanlg.servlet.SHA256</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateQuestionServlet</servlet-name>
        <servlet-class>hanlg.servlet.CreateQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadQuestionServlet</servlet-name>
        <servlet-class>hanlg.servlet.LoadQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchQuizServlet</servlet-name>
        <servlet-class>hanlg.servlet.SearchQuizServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateQuizServlet</servlet-name>
