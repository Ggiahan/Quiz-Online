    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationSevlet</servlet-name>
        <url-pattern>/RegistrationSevlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SHA256</servlet-name>
        <url-pattern>/SHA256</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateQuestionServlet</servlet-name>
        <url-pattern>/CreateQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadQuestionServlet</servlet-name>
        <url-pattern>/LoadQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchQuizServlet</servlet-name>
        <url-pattern>/SearchQuizServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateQuizServlet</servlet-name>
